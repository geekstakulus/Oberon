module GenericTest6

import 
	RL := Generic3<R>

type 
	R = record
			name: array 32 of char
			age: integer
		end

		
var
	l : RL.List
	r : R

	proc print( in elem: R )
	begin
		println(elem.name)
		println(elem.age)
	end print
begin
	new(l)
	r.name := "So long"
	r.age := 52
	l.setValue( r )
	println(l.value.name)
	println(l.value.age)
	l.print(print)
end GenericTest6
