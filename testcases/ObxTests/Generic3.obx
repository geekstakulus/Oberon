module Generic3<T>

type
	Element = record 
				value* : T 
		  end
   // illegal: Element2 = record(T) end
	
	List* = pointer to record (Element)
            next*  : List
        end
        
    Printer = proc( in elem: T );
        
   proc (in this: Element) print*(p: Printer)
   begin
	   p(this.value)
   end print
   
   proc (var this: Element) setValue*( in v: T )
   begin
	   this.value := v
	end setValue
   
end Generic3
