module Procs7

	// this code is not supported by Oberon+:
	
	proc Outer()
		var a,b,c: integer
		
		proc Mid1( i: integer )
		
			proc Inner( i: integer )
			begin
				a := a + i
				Mid2(11)
			end Inner
			
		begin
			b := b + i
			Inner(12)
