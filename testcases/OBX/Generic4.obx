module Generic4<T>

type
	Element = record 
				value* : T 
		  end
	
	List* = pointer to record (Element)
            next*  : List
        end
        
   proc (in this: Element) print*()
   begin
	   println(this.value)
   end print
   
   proc (in this: Element) setValue*( in v: T )
   begin
	   this.value := v
	end setValue
   
end Generic4
